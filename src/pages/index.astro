---
import "../assets/global.css";
import ZhHead from "../components/ZhHead.astro";
import { Link } from "@nextui-org/react";
const DemoList = [
  "/NoJs",
  "/AppSpllit",
  "/AppSpllitByAsync",
  "/ServerFetchData",
  "/Arknights",
];
---

<ZhHead />

<div class="container">
  <div>
    {
      DemoList.map((url) => {
        return (
          <div>
            <Link isBlock href={url}>
              go to demo - {url.split("/")[1]}
            </Link>
          </div>
        );
      })
    }
  </div>
</div>

<style lang="less">
  .container {
    width: 100vw;
    height: 100vh;
    display: flex;
    align-items: center;
    justify-content: center;
  }
</style>
