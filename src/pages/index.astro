---
import "../assets/global.css";
const DemoList = ["/NoJs", "/AppSpllit", "/AppSpllitByAsync"];
---

<head>
  <meta charset="utf-8" />
</head>

<div class="container">
  <div>
    {
      DemoList.map((url) => {
        return (
          <div>
            <a href={url}>go to demo - {url.split("/")[1]}</a>
          </div>
        );
      })
    }
  </div>
</div>

<style lang="less">
  .container {
    width: 100vw;
    height: 100vh;
    display: flex;
    align-items: center;
    justify-content: center;
  }
</style>
